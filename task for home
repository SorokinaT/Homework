def free_from_tests(group):
    student_free_from_tests = []
    for student_info in group:
        name = student_info[0]
        tasks = student_info[1]
        task_list = tasks.split(',')
        task_list_int = [int(x) for x in task_list]
        sum_of_tasks = len(task_list_int)
        # print(sum_of_tasks)
        sum_of_done_tasks = sum(task_list_int)
        # print(sum_of_done_tasks)
        if sum_of_tasks == sum_of_done_tasks:
            print("The students, who have done all tasks is %-20s" % name )
